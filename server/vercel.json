{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/nft/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/top-creators/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/trending-nfts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/notification/(.*)",
      "dest": "index.js"
    }
  ]
}